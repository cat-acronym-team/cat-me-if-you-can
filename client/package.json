{
  "name": "cat-me-if-you-can",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build:dev": "vite build --mode development",
    "build:test": "vite build --mode testing",
    "build:stage": "vite build --mode staging",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile src/theme/output/smui-light.css -i src/theme/light",
    "smui-theme-dark": "smui-theme compile src/theme/output/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/typography": "^14.0.0",
    "@playwright/test": "^1.30.0",
    "@smui-extra/badge": "^7.0.0-beta.0",
    "@smui/button": "^7.0.0-beta.0",
    "@smui/circular-progress": "^7.0.0-beta.0",
    "@smui/dialog": "^7.0.0-beta.0",
    "@smui/icon-button": "^7.0.0-beta.0",
    "@smui/menu": "^7.0.0-beta.0",
    "@smui/ripple": "^7.0.0-beta.0",
    "@smui/slider": "^7.0.0-beta.0",
    "@smui/textfield": "^7.0.0-beta.0",
    "@sveltejs/adapter-static": "^1.0.0",
    "@sveltejs/kit": "^1.0.1",
    "@types/d3-interpolate": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "smui-theme": "^7.0.0-beta.0",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.4",
    "svelte-preprocess": "^5.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.4.2"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.8",
    "@fontsource/roboto-mono": "^4.5.8",
    "@mdi/js": "^7.1.96",
    "d3-interpolate": "^3.0.1",
    "firebase": "^9.15.0"
  },
  "type": "module"
}
