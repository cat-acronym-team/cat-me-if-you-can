{
  "cSpell.words": [
    "Abdulaziz",
    "Asai",
    "Averbach",
    "Clusky",
    "Giantte",
    "Jumaah",
    "Muaad",
    "Salkeld",
    "Shunsuke",
    "smui"
  ],
  "conventionalCommits.scopes": ["client", "functions", "firestore"],
  "conventionalCommits.emojiFormat": "emoji",
  "eslint.workingDirectories": ["/client", "/functions"],
  "eslint.validate": ["javascript", "typescript", "html", "markdown", "svelte", "css"],
  "svelte.enable-ts-plugin": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.tabSize": 2,
  "files.eol": "\n",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  }
}
